{
  "name": "{{projectName}}",
  "version": "0.1.0",
  "private": true,
  "description": "{{projectDescription}}",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    {{#if useTests}}
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    {{/if}}
    "type-check": "tsc --noEmit",
    "setup": "npm install && npm run type-check",
    "start": "npm run dev",
    "clean": "rm -rf dist node_modules",
    "reinstall": "npm run clean && npm install"
  },
  "dependencies": {
    "@archbase/admin": "^3.0.1",
    "@archbase/advanced": "^3.0.0",
    "@archbase/components": "^3.0.0",
    "@archbase/core": "^3.0.0",
    "@archbase/data": "^3.0.0",
    "@archbase/layout": "^3.0.1",
    "@archbase/security": "^3.0.1",
    "@archbase/template": "^3.0.0",
    "@archbase/tools": "^3.0.0",
    "@mantine/core": "8.1.2",
    "@mantine/dates": "8.1.2",
    "@mantine/dropzone": "8.1.2",
    "@mantine/emotion": "8.1.2",
    "@mantine/hooks": "8.1.2",
    "@mantine/modals": "8.1.2",
    "@mantine/notifications": "8.1.2",
    "@mantine/spotlight": "8.1.2",
    "@tabler/icons-react": "^3.0.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tanstack/react-query": "^5.81.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.0.0",
    "axios": "^1.7.2",
    "clsx": "^2.1.1",
    "dayjs": "1.11.10",
    "final-form": "^4.20.10",
    "final-form-arrays": "^3.1.0",
    "i18next": "23.7.6",
    "i18next-browser-languagedetector": "^7.2.1",
    "lodash": "4.17.21",
    "react-final-form": "^6.5.9",
    "react-final-form-arrays": "^3.1.4",
    "react-i18next": "13.4.1",
    "react-router": "6.21.2",
    {{#if (includes features "notifications")}}
    "react-hot-toast": "^2.4.0",
    "socket.io-client": "^4.6.0",
    {{/if}}
    "uuid": "^9.0.1",
    "inversify": "6.2.0",
    "inversify-react": "^1.2.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.24.0",
    "@babel/plugin-transform-class-properties": "^7.24.0",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.2",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.5",
    "http2-proxy": "^5.0.53",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "^5.3.3",
    "vite": "^6.0.0",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-eslint": "^1.8.1",
    {{#if useTests}}
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@vitest/ui": "^1.1.0",
    "jsdom": "^23.0.0",
    "vitest": "^1.1.0"
    {{else}}
    "@vitest/ui": "^1.1.0",
    "jsdom": "^23.0.0",
    "vitest": "^1.1.0"
    {{/if}}
  },
  "peerDependencies": {
    "react": "^18.3.0 || ^19.0.0",
    "react-dom": "^18.3.0 || ^19.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "not dead",
    "not edge <= 18",
    "not ie 11",
    "not op_mini all"
  ],
  "pnpm": {
    "overrides": {
      "reflect-metadata": "0.2.2",
      "@tabler/icons-react": "^3.0.0"
    }
  }
}